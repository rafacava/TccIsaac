<p id="notice"><%= notice %></p>

<h1>Diagnosticos</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Principal fator</th>
      <th>Secundario fator</th>
      <th>Terciario fator</th>
      <th>Principal caracteristica</th>
      <th>Secundaria caracteristica</th>
      <th>Terciaria caracteristica</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @diagnosticos.each do |diagnostico| %>
      <tr>
        <td><%= diagnostico.nome %></td>
        <td><%= diagnostico.principal_fator.nome %></td>
        <td><%= diagnostico.secundario_fator.nome %></td>
        <td><%= diagnostico.terciario_fator.nome %></td>
        <td><%= diagnostico.principal_caracteristica.nome %></td>
        <td><%= diagnostico.secundaria_caracteristica.nome %></td>
        <td><%= diagnostico.terciaria_caracteristica.nome %></td>
        <td><%= link_to 'Show', diagnostico %></td>
        <td><%= link_to 'Edit', edit_diagnostico_path(diagnostico) %></td>
        <td><%= link_to 'Destroy', diagnostico, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Diagnostico', new_diagnostico_path %>
