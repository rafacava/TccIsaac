<p id="notice"><%= notice %></p>

<h1>Principal Caracteristicas</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @principal_caracteristicas.each do |principal_caracteristica| %>
      <tr>
        <td><%= principal_caracteristica.nome %></td>
        <td><%= link_to 'Show', principal_caracteristica %></td>
        <td><%= link_to 'Edit', edit_principal_caracteristica_path(principal_caracteristica) %></td>
        <td><%= link_to 'Destroy', principal_caracteristica, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Principal Caracteristica', new_principal_caracteristica_path %>
